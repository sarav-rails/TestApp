}
.block-more .btn-started:hover{text-decoration:none;}
#header .info{
	float:left;
	text-transform:uppercase;
	font-size:15px;
	font-weight:normal;
	margin:9px 0 0;
}
.menu{
	margin:11px 12px 0 0;	padding:0;	list-style:none;
	overflow:hidden;
	float:right;
	font-size:13px;
	line-height:15px;
	width:500px;
	text-align:right;
}
.menu li{
	display:inline-block;
	padding:0 2px 0 6px;
	background:url(<%= asset_path 'separator-menu.gif' %>) no-repeat 0 2px;
}
*+html .menu{margin:10px 8px 0 0}
*+html .menu li{
	display:inline;
	padding:0 6px 0 5px;
}
.menu li:first-child{
	background:url(<%= asset_path 'arrow.gif' %>) no-repeat 0 4px;
	padding-left:8px;
}
#main{
	width:100%;	position:relative;	margin:0 -9999px;	padding:28px 9999px 66px;
	background:url(<%= asset_path 'bg-main.jpg' %>) repeat-x;
}
.dashboard{padding:0 0 14px;}
.dashboard .row{
	width:100%;
	padding:0 0 16px;
	position:relative;
	z-index:10;
}
.dashboard .row:after{	content:"";	display:block;	clear:both;}
.dashboard .row-box{
	float:left;
	background:#fff;
	width:112px;
	border-radius:7px;	-moz-border-radius:7px;	-webkit-border-radius:7px;
	box-shadow:0 1px 0 #566e7a;
	-webkit-box-shadow:0 1px 0 #566e7a;	-moz-box-shadow:0 1px 0 #566e7a;	padding:22px 41px 21px 23px;
	margin:0 12px 0 3px;
	line-height:18px;
}
.dashboard .row-box p{margin:0 0 15px;}
.dashboard .row-box a{
	font-size:13px;
	text-decoration:underline;
}
.dashboard .row-box a:hover{text-decoration:none;}
.dashboard .row-box img{
	display:block;
	margin:0 0 7px;
}
.statistics{
	margin:0;	padding:0 1px 0 0;	list-style:none;
	float:left;
	text-align:center;
}
.statistics > li{
	float:left;
	background:#fff;
	width:109px;
	margin:0 12px 0 0;
	border-radius:7px;
	-moz-border-radius:7px;
	-webkit-border-radius:7px;
	position:relative;
}
.statistics > li:hover{
	box-shadow:0 1px 0 #566e7a, 0 0 4px #888;
	-webkit-box-shadow:0 1px 0 #566e7a, 0 0 4px #888;
	-moz-box-shadow:0 1px 0 #566e7a, 0 0 4px #888;
	border-radius:7px 7px 0 0;
	-moz-border-radius:7px 7px 0 0;
	-webkit-border-radius:7px 7px 0 0;
}
.statistics > li > a{
	display:block;
	width:87px;
	padding:14px 11px 17px;
	text-decoration:none;
	position:relative;
	z-index:10;
}
.statistics > li:hover > a{
	background:url(<%= asset_path 'bg-statistics.gif' %>) no-repeat 0 100%;
	padding-bottom:7px;
}
.statistics img{
	display:block;
	margin:0 auto 10px;
}
.statistics .count{
	font:34px/36px CalibriBold, Arial, Helvetica;
	display:block;
}
.statistics .count span{
	font:18px/20px CalibriRegular, Arial, Helvetica;
	color:#5a5a5a;
	display:block;
	margin:-1px 0 0;
}
.statistics ul{
	margin:0;	list-style:none;
	display:none;
	position:absolute;
	left:0;
	top:100%;
	background:#fff;
	width:87px;
	padding:0 11px 8px;
	border-radius:0 0 7px 7px;
	-moz-border-radius:0 0 7px 7px;
	-webkit-border-radius:0 0 7px 7px;
	box-shadow:0 0 4px #566e7a;
}
.statistics li:hover ul{display:block;}
.statistics .count2{
	font:20px/22px MyriadProSemibold, Arial, Helvetica;
	display:block;
	border-top:1px solid #bcdfed;
	padding:7px 0;
	margin:0;
}
.statistics .count2 span{
	font:14px/16px CalibriRegular, Arial, Helvetica;
	color:#5a5a5a;
	display:block;
	margin:2px 0 0;
}
