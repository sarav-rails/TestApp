html{min-width:980px;}
body{
	margin:0;
	color:#25363d;
	font:16px/20px CalibriRegular, Arial, Helvetica, sans-serif;
	background:#f1f1f1;
	min-width:980px;
}
img{border-style:none;}
a{
	text-decoration:none;
	color:#000;
}
a:hover{text-decoration:underline;}
input,
textarea,
select{
	font:100% Arial, Helvetica, sans-serif;
	vertical-align:middle;
	color:#000;
}
form,
fieldset{
	margin:0;
	padding:0;
	border-style:none;
}
header, footer, article, section, hgroup, nav, figure, figcaption{display:block;}
figure{
	margin:0;
	padding:0;
}
.inner-page4{background:url(<%= asset_path 'bg-body.gif' %>);}
#wrapper{
	width:100%;
	position:relative;
	overflow:hidden;
}
.inner-page4 .w2{
	width:100%;
	position:relative;	margin:0 -9999px;	padding:0 9999px;
	background:url(<%= asset_path 'bg-footer.jpg' %>) no-repeat 50% 100%;
}
.w1{
	width:980px;
	margin:0 auto;
}
#header{
	width:100%;	position:relative;	margin:0 -9999px;	padding:13px 9999px;
	background:#73b0d0 url(<%= asset_path 'bg-header.gif' %>) repeat-x;
	border-bottom:1px solid #5995b4;
	color:#fff;
	overflow:hidden;
}
.inner-page4 #header{
	border-bottom:1px solid #fff;
	padding:0 9999px;
	background:url(<%= asset_path 'bg-page4-header.gif' %>) repeat-x;
}
.inner-page4 .header-holder{background:url(<%= asset_path 'bg-header-holder.png' %>) no-repeat 50% 0;}
#header a{color:#fff;}
.logo{
	float:left;
	width:153px;
	height:30px;
	background:url(<%= asset_path 'logo.gif' %>);
	text-indent:-9999px;
	overflow:hidden;
	margin:0 2px 0 5px;
}
.logo a{
	display:block;
	height:100%;
}
.inner-page4 .logo{
	width:161px;
	height:31px;
	margin:0 0 0 42px;
	background:url(<%= asset_path 'logo.png' %>) no-repeat;
}
#header .panel{
	width:100%;	overflow:hidden;
	padding:29px 0 7px;
	background:url(<%= asset_path 'bg-panel.gif' %>) repeat-x 0 100%;
}
#header .bar{
	width:100%;	overflow:hidden;
}
#header .gallery-holder{
	width:100%;	overflow:hidden;
	position:relative;
}
#header .gallery{
	position:relative;
	z-index:1;
	width:961px;
	margin:0 auto;
	height:410px;
	overflow:hidden;
}
#header .gallery-list{	margin:0;	padding:0;	list-style:none;
	width:9999px;
	position:relative;	overflow:hidden;}
#header .gallery-list li{
	float:left;
	width:929px;
	height:394px;
	position:relative;
	padding:16px 0 0 32px;
}
#header .gallery-list .image-holder{
	position:absolute;
	bottom:0;
	right:45px;
}
#header .gallery-list img{vertical-align:top;}
#header .gallery-list{
	color:#fff;
	text-shadow: 0 0 1px #000;
	font:28px/34px 'CalibriRegular', Arial, Helvetica, sans-serif;
}
#header .gallery-list h2{
	margin:0;
	font:42px/40px 'CalibriBold', Arial, Helvetica, sans-serif;
}
#header .gallery-list p{margin:0;}
#header .gallery-holder .prev,
#header .gallery-holder .next{
	width:54px;
	height:85px;
	position:absolute;
	top:246px;
	left:0;
	text-indent:-9999px;
	overflow:hidden;
	cursor:pointer;
	z-index:2;
	background:url(<%= asset_path 'gallery-arrows.png' %>) no-repeat;
}
#header .gallery-holder .next{
	left:auto;
	right:0;
	background-position:100% 0;
}
#header .gallery-holder .prev:hover{background-position:0 100%;}
#header .gallery-holder .next:hover{background-position:100% 100%;}
.registration-form{
	position:absolute;
	left:57px;
	top:153px;
	width:230px;
	margin:0;
	padding:13px 33px 18px;
	background:url(<%= asset_path 'bg-body.gif' %>);
	border:1px solid #fff;
	border-left:1px solid #607e8d;
	border-bottom:1px solid #607e8d;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	z-index:5;
}
.registration-form h3{
	margin:0 0 12px;
	color:#4b4b4b;
