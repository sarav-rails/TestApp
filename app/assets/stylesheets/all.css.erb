	background:url(<%= asset_path 'btn-submit.gif' %>) no-repeat;
}
.registration-form span{
	font-size:12px;
	text-align:center;
	color:#626262;
	display:block;
	text-shadow: 1px 1px 1px #fff;
}
#header .registration-form a{
	color:#626262;
	text-decoration:underline;
}
#header .registration-form a:hover{text-decoration:none;}
.inner-page4 #main{
	background:none;
	padding:0 9999px 402px;
}
.inner-page4 .main-holder{
	width:940px;
	padding:29px 20px 0;
}
